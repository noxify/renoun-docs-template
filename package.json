{
  "name": "renoun-docs-template",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "renoun next build && pnpm generate-pagefind",
    "deps:check": "pnpm outdated -r",
    "deps:update": "pnpm update -i -L -r",
    "dev": "renoun next dev",
    "format": "prettier --check . --ignore-path .gitignore --ignore-path .prettierignore",
    "format:fix": "prettier --check --write . --ignore-path .gitignore --ignore-path .prettierignore",
    "generate-pagefind": "pagefind --site out --output-path out/pagefind",
    "info": "next info",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:links": "node --import ./src/register.js --import tsx/esm src/link-check.ts",
    "preview": "tsx src/localserver.ts",
    "renoun-test": "tsx src/test.ts",
    "typecheck": "tsc --noEmit",
    "ui": "shadcn"
  },
  "prettier": "./prettier.config.js",
  "dependencies": {
    "@getcanary/web": "1.0.12",
    "@giscus/react": "3.1.0",
    "@hapi/bourne": "3.0.0",
    "@mdx-js/loader": "3.1.0",
    "@mdx-js/node-loader": "3.1.0",
    "@mdx-js/react": "3.1.0",
    "@next/mdx": "15.1.7",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-collapsible": "1.1.3",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-dropdown-menu": "2.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "1.1.2",
    "@radix-ui/react-tabs": "1.1.3",
    "@renoun/mdx": "1.3.2",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "lucide-react": "0.475.0",
    "mermaid": "11.4.1",
    "multimatch": "7.0.0",
    "next": "15.1.7",
    "next-themes": "0.4.4",
    "railroad-diagrams": "1.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-element-to-jsx-string": "17.0.0",
    "rehype-mdx-import-media": "^1.2.0",
    "renoun": "8.3.2",
    "tailwind-merge": "3.0.1",
    "tailwindcss-animate": "1.0.7",
    "tm-themes": "1.9.12",
    "ts-dot-prop": "2.1.4",
    "zod": "3.24.2",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.6",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@next/eslint-plugin-next": "15.1.7",
    "@tailwindcss/postcss": "^4.0.6",
    "@tailwindcss/typography": "0.5.16",
    "@types/mdx": "2.0.13",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/serve-handler": "6.1.4",
    "eslint": "9.20.1",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-import-x": "4.6.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-package-json": "0.26.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "mdast-util-to-string": "4.0.0",
    "next-validate-link": "1.4.1",
    "pagefind": "1.3.0",
    "postcss": "8.5.2",
    "prettier": "3.5.0",
    "prettier-plugin-tailwindcss": "0.6.11",
    "remark-parse": "11.0.0",
    "serve-handler": "6.1.6",
    "shadcn": "2.3.0",
    "source-map-support": "0.5.21",
    "tailwindcss": "4.0.6",
    "tsx": "4.19.2",
    "type-fest": "4.34.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.0",
    "unified": "11.0.5"
  },
  "packageManager": "pnpm@10.3.0",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "minimist@<0.2.1": ">=0.2.1",
      "minimist@<0.2.4": ">=0.2.4",
      "path-to-regexp@<3.3.0": ">=3.3.0"
    }
  }
}
