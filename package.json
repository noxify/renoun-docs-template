{
  "name": "renoun-docs-template",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "renoun next build && pnpm generate-pagefind",
    "deps:check": "pnpm outdated -r",
    "deps:update": "pnpm update -i -L -r",
    "dev": "renoun next dev",
    "format": "prettier --check . --ignore-path .gitignore",
    "format:fix": "prettier --check --write . --ignore-path .gitignore",
    "generate-pagefind": "pagefind --site out --output-path out/pagefind",
    "info": "next info",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "preview": "tsx src/localserver.ts",
    "typecheck": "tsc --noEmit",
    "ui": "shadcn"
  },
  "prettier": "./prettier.config.js",
  "dependencies": {
    "@getcanary/web": "1.0.9",
    "@giscus/react": "3.0.0",
    "@hapi/bourne": "3.0.0",
    "@mdx-js/loader": "3.0.1",
    "@mdx-js/react": "3.0.1",
    "@next/mdx": "14.2.15",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-slot": "1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@renoun/mdx": "1.1.0",
    "@types/mdx": "2.0.13",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "lucide-react": "0.452.0",
    "multimatch": "7.0.0",
    "next": "14.2.15",
    "next-themes": "0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "renoun": "5.5.0",
    "restyle": "2.4.0",
    "tailwind-merge": "2.5.3",
    "tailwindcss-animate": "1.0.7",
    "tm-themes": "1.8.8",
    "ts-dot-prop": "2.1.4",
    "zod": "3.23.8",
    "zustand": "5.0.0-rc.2"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.0",
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@next/eslint-plugin-next": "14.2.15",
    "@tailwindcss/typography": "0.5.15",
    "@types/mdx": "2.0.13",
    "@types/node": "22.7.5",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@types/serve-handler": "6.1.4",
    "eslint": "9.12.0",
    "eslint-config-next": "14.2.15",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-package-json": "0.15.3",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "rc",
    "pagefind": "1.1.1",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "serve-handler": "6.1.5",
    "shadcn": "2.1.0",
    "tailwindcss": "3.4.13",
    "tsx": "4.19.1",
    "typescript": "5.6.3",
    "typescript-eslint": "8.8.1"
  },
  "pnpm": {
    "overrides": {
      "minimist@<0.2.1": ">=0.2.1",
      "minimist@<0.2.4": ">=0.2.4",
      "path-to-regexp@<3.3.0": ">=3.3.0"
    }
  }
}
